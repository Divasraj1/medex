{
    "name":"medex",
    "version":"1.0",
    "manifest_version": 2,
    "description":"Add a medicine reminder",
    "icons":{
        "128": "icon128.PNG"
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "alarms",
        "notifications",
        "storage"
    ],
    "background": {
        "scripts": ["popup.js"],
        "persistent": false
    }

}